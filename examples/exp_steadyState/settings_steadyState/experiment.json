{
  "basics": {
    "config_files": {
      "forcing": "forcing.json",
      "model_structure": "model_structure.json",
      "optimization": "optimization.json"
    },
    "domain": "FLUXNET_Site",
    "name": "SteadyState",
    "time": {
      "date_begin": "2000-01-01",
      "date_end": "2017-12-31",
      "temporal_resolution": "day",
      "timesteps_in_day": 1,
      "timesteps_in_year": 365
    }
  },
  "exe_rules": {
    "input_array_type": "keyed_array",
    "input_data_backend": "netcdf",
    "land_output_type": "array",
    "longtuple_size": 10,
    "model_array_type": "static_array",
    "model_number_type": "Float32",
    "parallelization": "threads",
    "tolerance": 1.0e-2,
    "yax_max_cache": 2e9
  },
  "flags": {
    "calc_cost": false,
    "catch_model_errors": false,
    "debug_model": false,
    "inline_update": false,
    "run_forward": true,
    "run_optimization": false,
    "save_info": true,
    "spinup": {
      "load_spinup": false,
      "run_spinup": true,
      "save_spinup": false,
      "spinup_TEM": true,
      "store_spinup": false
    },
    "use_forward_diff": false
  },
  "model_output": {
    "all": true,
    "depth_dimensions": {
      "d_cEco": 8,
      "d_snow": "snowW",
      "d_soil": "soilW"
    },
    "format": "zarr",
    "output_array_type": "array",
    "path": null,
    "plot_model_output": false,
    "save_single_file": true,
    "variables": {
      "fluxes.evapotranspiration": null,
      "fluxes.gpp": null,
      "fluxes.nee": null,
      "fluxes.runoff": null,
      "fluxes.surface_runoff": null,
      "fluxes.transpiration": null,
      "pools.cEco": "d_cEco",
      "pools.snowW": "d_snow",
      "pools.soilW": "d_soil",
      "states.water_balance": null
    }
  },
  "model_spinup": {
    "paths": {
      "restart_file_in": null,
      "restart_file_out": null
    },
    "sequence": [
      {
        "forcing": "day_MSC",
        "n_repeat": 15,
        "spinup_mode": "all_forward_models"
      },
      {
        "forcing": "all_years",
        "n_repeat": 1,
        "spinup_mode": "all_forward_models"
      }
    ]
  }
}
