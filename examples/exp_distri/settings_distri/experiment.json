{
  "basics": {
    "config_files": {
      "forcing": "forcing.json",
      "model_structure": "model_structure.json",
      "optimization": "optimization.json"
    },
    "domain": "FLUXNET",
    "name": "Distributed",
    "time": {
      "date_begin": "1979-01-01",
      "date_end": "2017-12-31",
      "temporal_resolution": "day"
    }
  },
  "exe_rules": {
    "input_array_type": "keyed_array",
    "input_data_backend": "zarr",
    "land_output_type": "array",
    "longtuple_size": null,
    "model_array_type": "static_array",
    "model_number_type": "Float32",
    "parallelization": "threads",
    "tolerance": 1.0e-3,
    "yax_max_cache": 2e9
  },
  "flags": {
    "calc_cost": true,
    "catch_model_errors": false,
    "debug_model": false,
    "filter_nan_pixels": false,
    "inline_update": false,
    "run_forward": true,
    "run_optimization": false,
    "save_info": true,
    "spinup_TEM": true,
    "store_spinup": false,
    "use_forward_diff": false
  },
  "model_output": {
    "depth_dimensions": {
      "d_cEco": 8,
      "d_snow": "snowW",
      "d_soil": "soilW"
    },
    "format": "nc",
    "output_array_type": "array",
    "path": null,
    "plot_model_output": false,
    "save_single_file": false,
    "variables": {
      "fluxes.evapotranspiration": null,
      "fluxes.gpp": null,
      "fluxes.runoff": null,
      "fluxes.surface_runoff": null,
      "fluxes.transpiration": null,
      "pools.snowW": "d_snow",
      "pools.soilW": "d_soil",
      "diagnostics.water_balance": null
    }
  },
  "model_spinup": {
    "restart_file": null,
    "sequence": [
      {
        "forcing": "day_MSC",
        "n_repeat": 100,
        "spinup_mode": "all_forward_models"
      },
      {
        "forcing": "all_years",
        "n_repeat": 1,
        "spinup_mode": "all_forward_models",
        "options": {
          "absolute_tolerance": 1e-2,
          "pools": [
            "cEco"
          ],
          "relative_tolerance": 1e-2,
          "time_jump": 200.0
        }
      }
    ]
  }
}