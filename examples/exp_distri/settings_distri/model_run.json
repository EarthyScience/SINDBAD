{
  "experiment_flags": {
    "catch_model_errors": false,
    "debug_model": false,
    "run_forward": true,
    "run_forward_and_cost": true,
    "run_optimization": true,
    "run_update_models": false,
    "save_info": true,
    "spinup": {
      "do_spinup": true,
      "load_spinup": false,
      "run_spinup": true,
      "save_spinup": false,
      "store_spinup_history": false
    }
  },
  "experiment_rules": {
    "data_type": "Float32",
    "forward_diff": false,
    "input_data_backend": "zarr",
    "model_array_type": "staticarray",
    "parallelization": "threads",
    "tolerance": 1.0e-3,
    "verbosity": 5,
    "yax_max_cache": 2e9
  },
  "experiment_time": {
    "date_begin": "2005-01-01",
    "date_end": "2017-12-31",
    "timestep": "day",
    "timesteps_in_day": 1,
    "timesteps_in_year": 365.0
  },
  "output": {
    "all": false,
    "depth_dimensions": {
      "d_cEco": 8,
      "d_snow": "snowW",
      "d_soil": "soilW"
    },
    "format": "nc",
    "output_array_type": "array",
    "path": null,
    "save_single_file": false,
    "variables": {
      "fluxes.evapotranspiration": null,
      "fluxes.gpp": null,
      "fluxes.runoff": null,
      "fluxes.surface_runoff": null,
      "fluxes.transpiration": null,
      "pools.snowW": "d_snow",
      "pools.soilW": "d_soil",
      "states.water_balance": null
    }
  },
  "spinup": {
    "differential_eqn": {
      "absolute_tolerance": 1e-2,
      "pools": ["cEco"],
      "relative_tolerance": 1e-2,
      "time_jump": 200.0
    },
    "paths": {
      "restart_file_in": null,
      "restart_file_out": null
    },
    "sequence": [
      {
        "forcing": "day_msc",
        "n_repeat": 100,
        "spinup_mode": "forward",
        "stop_function": null
      },
      {
        "forcing": "all_years",
        "n_repeat": 1,
        "spinup_mode": "forward",
        "stop_function": null
      }
    ]
  }
}
