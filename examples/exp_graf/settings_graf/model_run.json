{
  "time": {
    "model_time_step": "day",
    "start_date": "2006-01-01",
    "end_date": "2015-12-31",
    "timesteps_in_day": 1,
    "timesteps_in_year": 365
  },
  "mapping": {
    "yaxarray": ["id"],
    "run_ecosystem": ["time"],
    "parallelization": "threads"
  },
  "flags": {
    "save_info": true,
    "debug_model": false,
    "catch_model_errors": false,
    "run_forward": true,
    "run_update_models": false,
    "run_optimization": false,
    "run_forward_and_cost": true,
    "spinup": {
      "do_spinup": true,
      "load_spinup": false,
      "run_spinup": true,
      "save_spinup": false,
      "store_spinup_history": false
    }
  },
  "rules": {
    "verbosity": 5,
    "model_array_type": "staticarray",
    "forward_diff": false,
    "data_type": "Float32",
    "input_data_backend": "netcdf",
    "tolerance": 1.0e-3,
    "yax_max_cache": 2e9
  },
  "output": {
    "save_single_file": false,
    "output_array_type": "array",
    "all": true,
    "path": null,
    "format": "zarr",
    "depth_dimensions": {
      "d_soil": "soilW",
      "d_cEco": 8,
      "d_snow": "snowW"
    },
    "variables": {
      "gppDemand.gpp_demand": null,
      "gppAirT.gpp_f_airT": null,
      "gppVPD.gpp_f_vpd": null,
      "gppDemand.gpp_f_climate": null,
      "gppPotential.gpp_potential": null,
      "states.fAPAR": null,
      "fluxes.evapotranspiration": null,
      "fluxes.transpiration": null,
      "fluxes.runoff": null,
      "fluxes.surface_runoff": null,
      "fluxes.gpp": null,
      "pools.soilW": "d_soil",
      "states.water_balance": null
    }
  },
  "spinup": {
    "sequence": [
      {
        "spinup_mode": "forward",
        "forcing": "full",
        "stop_function": null,
        "n_repeat": 20
      }
    ],
    "differential_eqn": {
      "pools": ["cEco"],
      "time_jump": 200.0,
      "relative_tolerance": 1e-2,
      "absolute_tolerance": 10.0
    },
    "paths": {
      "restart_file_in": null,
      "restart_file_out": null
    }
  }
}
