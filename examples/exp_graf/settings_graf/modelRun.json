{
	"time":
	{
		"model_time_step": 			"daily",
		"start_date": 				"2006-01-01",
		"end_date": 				"2015-12-31",
		"timesteps_in_day": 		1,
		"timesteps_in_year": 		365
	},
	"mapping": {
		"yaxarray": ["id"],
		"run_ecosystem": ["time"],
		"parallelization": "threads"
	},
	"flags":
	{
		"save_info": 			true,
		"debug_model": 			false, 
		"catch_model_errors": 	false,
		"run_spinup": 			true,
		"run_forward": 			true,
		"run_update_models": 	false,
		"run_optimization": 	true,
		"run_forward_and_cost":	true
	},
	"rules":
	{
		"verbosity": 5,
		"model_array_type": "staticarray",
		"forward_diff": false,
		"data_type": "Float32",
		"data_backend": "yaxarray",
		"tolerance": 1.0e-3,
		"yax_max_cache": 2e9
	},
	"output": {
		"output_array_type": "array",		
		"all": true,
		"path": null,
		"format": ".zarr",
		"depth_dimensions":{
			"d_soil": "soilW",
			"d_cEco": 8,
			"d_snow": "snowW"
		},
		"variables": {
			"gppDemand.gpp_demand": null,
			"gppAirT.gpp_f_airT": null,
			"gppVPD.gpp_f_vpd": null,
			"gppDemand.gpp_f_climate": null,
			"gppPotential.gpp_potential": null,
			"states.fAPAR": null,
			"fluxes.evapotranspiration": null,
			"fluxes.transpiration": null,
			"fluxes.runoff": null,
			"fluxes.surface_runoff": null,
			"fluxes.gpp": null,
			"pools.soilW": "d_soil",
			"waterBalance.water_balance": null
		}
	}
}
