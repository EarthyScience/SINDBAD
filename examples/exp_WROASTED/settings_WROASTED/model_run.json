{
  "time": {
    "model_time_step": "day",
    "start_date": "1979-01-01",
    "end_date": "2017-12-31",
    "timesteps_in_day": 1,
    "timesteps_in_year": 365
  },
  "mapping": {
    "yaxarray": [],
    "run_ecosystem": ["time", "longitude", "latitude"],
    "parallelization": "qbmap"
  },
  "flags": {
    "save_info": true,
    "debug_model": false,
    "catch_model_errors": false,
    "run_forward": true,
    "run_update_models": false,
    "run_optimization": true,
    "run_forward_and_cost": true,
    "spinup": {
      "do_spinup": true,
      "load_spinup": false,
      "run_spinup": true,
      "save_spinup": false,
      "store_spinup_history": false
    }
  },
  "rules": {
    "verbosity": 5,
    "model_array_type": "staticarray",
    "forward_diff": false,
    "data_type": "Float32",
    "input_data_backend": "netcdf",
    "tolerance": 1.0e-2,
    "yax_max_cache": 2e9
  },
  "output": {
    "save_single_file": true,
    "output_array_type": "array",
    "all": true,
    "path": null,
    "format": "zarr",
    "depth_dimensions": {
      "d_soil": "soilW",
      "d_tws": "TWS",
      "d_cEco": 8,
      "d_snow": "snowW"
    },
    "variables": {
      "autoRespirationAirT.auto_respiration_f_airT": null,
      "cTauSoilT.c_eco_k_soilT": null,
      "cTauSoilW.c_eco_k_soilW": "d_cEco",
      "fluxes.evapotranspiration": null,
      "fluxes.transpiration": null,
      "fluxes.runoff": null,
      "fluxes.surface_runoff": null,
      "fluxes.base_runoff": null,
      "fluxes.snow_melt": null,
      "fluxes.gpp": null,
      "fluxes.auto_respiration": null,
      "fluxes.hetero_respiration": null,
      "fluxes.nee": null,
      "fluxes.gw_recharge": null, 
      "gppDemand.gpp_climate_stressors": 4,
      "gppSoilW.gpp_f_soilW": null,
      "WUE.AoE": null,
      "pools.TWS": "d_tws",
      "pools.soilW": "d_soil",
      "pools.groundW": 1,
      "pools.snowW": 1,
      "pools.cEco": "d_cEco",
      "rootWaterEfficiency.root_water_efficiency": "d_soil",
      "states.fAPAR": null,
      "states.c_eco_k": "d_cEco",
      "states.c_allocation": "d_cEco",
      "states.c_eco_efflux": "d_cEco",
      "states.c_eco_influx": "d_cEco",
      "states.c_eco_out": "d_cEco",
      "states.c_eco_flow": "d_cEco",
      "cFlow.leaf_to_reserve_frac": null,
      "cFlow.root_to_reserve_frac": null,
      "cFlow.reserve_to_leaf_frac": null,
      "cFlow.reserve_to_root_frac": null,
      "cFlow.k_shedding_leaf_frac": null,
      "cFlow.k_shedding_root_frac": null,
      "states.frac_snow": null,
      "states.c_flow_A_vec": 10,
      "transpirationSupply.transpiration_supply": null,
      "vegAvailableWater.PAW": "d_soil",
      "waterBalance.water_balance": null
    }
  },
  "spinup": {
    "sequence": [
      {
        "spinup_mode": "forward",
        "forcing": "recycleMSC",
        "stop_function": null,
        "n_repeat": 200
      }
    ],
    "differential_eqn": {
      "pools": ["cEco"],
      "time_jump": 2000.0,
      "relative_tolerance": 1e-2,
      "absolute_tolerance": 1
    },
    "paths": {
      "restart_file_in": null,
      "restart_file_out": null
    }
  }
}
