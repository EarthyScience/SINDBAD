{
  "basics": {
    "config_files": {
      "forcing": "forcing.json",
      "model_structure": "model_structure.json",
      "optimization": "optimization.json"
    },
    "domain": "FLUXNET",
    "name": "WROASTED",
    "time": {
      "date_begin": "1979-01-01",
      "date_end": "2017-12-31",
      "temporal_resolution": "day",
      "timesteps_in_day": 1,
      "timesteps_in_year": 365
    }
  },
  "exe_rules": {
    "data_type": "Float32",
    "input_array_type": "keyed_array",
    "input_data_backend": "netcdf",
    "model_array_type": "static_array",
    "parallelization": "threads",
    "tolerance": 1.0e-2,
    "yax_max_cache": 2e9
  },
  "flags": {
    "calc_cost": true,
    "catch_model_errors": false,
    "debug_model": false,
    "inline_update": false,
    "run_forward": true,
    "run_optimization": true,
    "save_info": true,
    "spinup": {
      "load_spinup": false,
      "run_spinup": true,
      "save_spinup": false,
      "spinup_TEM": true,
      "store_spinup": false
    },
    "use_forward_diff": false
  },
  "model_output": {
    "all": true,
    "depth_dimensions": {
      "d_cEco": 8,
      "d_snow": "snowW",
      "d_soil": "soilW",
      "d_tws": "TWS"
    },
    "format": "zarr",
    "output_array_type": "array",
    "path": null,
    "save_single_file": true,
    "variables": {
      "autoRespirationAirT.auto_respiration_f_airT": null,
      "cFlow.k_shedding_leaf_frac": null,
      "cFlow.k_shedding_root_frac": null,
      "cFlow.leaf_to_reserve_frac": null,
      "cFlow.reserve_to_leaf_frac": null,
      "cFlow.reserve_to_root_frac": null,
      "cFlow.root_to_reserve_frac": null,
      "cTauSoilT.c_eco_k_f_soilT": null,
      "cTauSoilW.c_eco_k_f_soilW": "d_cEco",
      "fluxes.auto_respiration": null,
      "fluxes.base_runoff": null,
      "fluxes.evapotranspiration": null,
      "fluxes.gpp": null,
      "fluxes.gw_recharge": null,
      "fluxes.hetero_respiration": null,
      "fluxes.nee": null,
      "fluxes.runoff": null,
      "fluxes.snow_melt": null,
      "fluxes.surface_runoff": null,
      "fluxes.transpiration": null,
      "gppDemand.gpp_climate_stressors": 4,
      "gppSoilW.gpp_f_soilW": null,
      "pools.cEco": "d_cEco",
      "pools.groundW": 1,
      "pools.snowW": 1,
      "pools.soilW": "d_soil",
      "pools.TWS": "d_tws",
      "states.c_allocation": "d_cEco",
      "states.c_eco_efflux": "d_cEco",
      "states.c_eco_flow": "d_cEco",
      "states.c_eco_influx": "d_cEco",
      "states.c_eco_k": "d_cEco",
      "states.c_eco_out": "d_cEco",
      "states.c_flow_A_vec": 10,
      "states.fAPAR": null,
      "states.frac_snow": null,
      "states.PAW": "d_soil",
      "states.root_water_efficiency": "d_soil",
      "states.transpiration_supply": null,
      "states.water_balance": null,
      "WUE.AoE": null
    }
  },
  "model_spinup": {
    "differential_eqn": {
      "absolute_tolerance": 1,
      "pools": ["cEco"],
      "relative_tolerance": 1e-2,
      "time_jump": 2000.0
    },
    "paths": {
      "restart_file_in": null,
      "restart_file_out": null
    },
    "sequence": [
      {
        "forcing": "first_year",
        "n_repeat": 200,
        "spinup_mode": "all_forward_models",
        "stop_function": null
      }
    ]
  }
}
