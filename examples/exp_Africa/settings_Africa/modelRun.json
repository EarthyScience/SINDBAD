{
	"1.c":	"flags for running optimization (runOpti), forward (runForward), forward run  per year (runForwardYearly), or forward run with model cost (calcCost)",
	"2.c":	"information related to model time steps",
	"3.c": "output and datatype",

	"time":
	{
		"step": 				"daily",
		"sDate": 				"2010-01-01",
		"eDate": 				"2010-12-31",
		"nYears": 				12,
		"nStepsDay": 			1,
		"nStepsYear": 			365
	},
	"mapping": {
		"yaxarray": ["longitude", "latitude"],
		"runEcosystem": ["time"]
	},
	"flags":
	{
		"runForwardYearly": 	false,
		"runForward": 			true,
		"runUpdateModels": 		false,
		"runOpti": 				true,
		"calcCost":				true
	},
	"rules":
	{
		"verbosity": 5,
		"dataType": "Float64",
		"tolerance": 1.0e-3
	},
	"output": {
		"all": false,
		"dirPath": "sandbox/output/",
		"dataFormat": ".nc",
		"variables": {
			"write": [
					"fluxes.gpp",
					"gppDemand.gppE",
					"states.fAPAR"
				],
			"store": []
		}
	}
}
