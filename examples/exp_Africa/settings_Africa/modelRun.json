{

	"time":
	{
		"step": 				"daily",
		"sDate": 				"2010-01-01",
		"eDate": 				"2010-12-31",
		"nYears": 				12,
		"nStepsDay": 			1,
		"nStepsYear": 			365
	},
	"mapping": {
		"yaxarray": ["longitude", "latitude"],
		"runEcosystem": ["time"],
		"parallelization": "threads"
	},
	"flags":
	{
		"saveInfo": 			true,
		"catchErrors": 			true,
		"runForward": 			true,
		"runUpdateModels": 		false,
		"runOpti": 				false,
		"calcCost":				false
	},
	"rules":
	{
		"verbosity": 5,
		"data_type": "Float64",
		"data_backend": "yaxarray",
		"tolerance": 1.0e-3,
		"yax_max_cache": 5e9
	},
	"output": {
		"all": false,
		"path": "africa",
		"format": ".zarr",
		"depth_dimensions":{
			"d_soil": "soilW",
			"d_cEco": 8,
			"d_snow": "snowW"
		},
		"variables": {
					"fluxes.gpp": null,
					"gppDemand.gppE": null,
					"states.fAPAR": null
		}
	}
}
