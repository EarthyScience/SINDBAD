{
  "algorithm": "CMAEvolutionStrategy_CMAES",
  "land_output_type": "array",
  "model_parameter_default": {
    "distribution": ["normal", [0, 1]],
    "is_ml": false
  },
  "model_parameters_to_optimize": [
    "gppPotential,εmax",
    "gppAirT,Topt",
    "gppAirT,ToptA",
    "gppAirT,ToptB",
    "gppVPD,κ",
    "gppVPD,Cκ",
    "gppVPD,Cm"
  ],
  "multi_constraint_method": "sum",
  "multi_objective_algorithm": false,
  "observational_constraints": ["gpp"],
  "observations": {
    "default_cost": {
      "aggr_order": "timespace",
      "area_weight": false,
      "cost_metric": "nseinv",
      "cost_weight": 1.0,
      "min_data_points": 1,
      "spatial_aggr": "cat",
      "spatial_cost_aggr": "spatial",
      "temporal_aggr": "day",
      "temporal_aggr_func": "nanMean",
      "temporal_aggr_obs": false
    },
    "default_observation": {
      "additive_unit_conversion": false,
      "bounds": null,
      "data_path": "../data/BE-Vie.1979.2017.daily.nc",
      "is_categorical": false,
      "name_short": null,
      "sindbad_unit": null,
      "source_product": "",
      "source_to_sindbad_unit": 1.0,
      "source_unit": null,
      "source_variable": null,
      "space_time_type": "spatiotemporal"
    },
    "use_quality_flag": true,
    "use_uncertainty": false,
    "variables": {
      "agb": {
        "cost_options": {
          "cost_metric": "nmae1r",
          "temporal_aggr": "mean",
          "temporal_aggr_obs": true
        },
        "data": {
          "bounds": [0.0, 100000.0],
          "name_short": "Above-ground biomass",
          "sindbad_unit": "gC m-2",
          "source_unit": "gC m-2",
          "source_variable": "agb_merged"
        },
        "model_full_var": "pools.cVegWood",
        "unc": {
          "bounds": [0.0, 400.0],
          "data_path": null,
          "source_variable": "NEE_RANDUNC"
        }
      },
      "evapotranspiration": {
        "cost_options": {
          "cost_metric": "nseinv"
        },
        "data": {
          "bounds": [0.0, 100.0],
          "is_categorical": false,
          "name_short": "Evapotranspiration",
          "sindbad_unit": "mm day-1",
          "source_to_sindbad_unit": 0.4081632653,
          "source_unit": "MJ m-2 d-1",
          "source_variable": "LE"
        },
        "model_full_var": "fluxes.evapotranspiration",
        "qflag": {
          "bounds": [0.85, 1.0],
          "data_path": null,
          "source_variable": "LE_QC_merged"
