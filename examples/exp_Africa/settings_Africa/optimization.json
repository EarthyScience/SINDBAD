        "unc": {
          "bounds": [0.0, 1.0],
          "data_path": null,
          "source_variable": "NEE_RANDUNC"
        }
      },
      "nee": {
        "cost_options": {
          "cost_metric": "nseinv"
        },
        "data": {
          "bounds": [-100.0, 100.0],
          "name_short": "Net Ecosystem Exchange",
          "sindbad_unit": "gC m-2 day-1",
          "source_unit": "gC m-2 day-1",
          "source_variable": "NEE"
