        "unc": {
          "bounds": [0.0, 100.0],
          "data_path": null,
          "source_to_sindbad_unit": 0.4081632653,
          "source_variable": "LE_RANDUNC"
        }
      },
      "gpp": {
        "cost_options": {
          "cost_metric": "nseinv"
        },
        "data": {
          "bounds": [0.0, 100.0],
          "name_short": "Gross Primary Productivity",
          "sindbad_unit": "gC m-2 day-1",
          "source_unit": "gC m-2 day-1",
          "source_variable": "GPP_NT"
        },
        "model_full_var": "fluxes.gpp",
        "qflag": {
          "bounds": [0.85, 1.0],
          "data_path": null,
          "source_variable": "GPP_QC_NT_merged"
