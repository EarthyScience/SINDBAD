{
  "basics": {
    "config_files": {
      "forcing": "forcing.json",
      "model_structure": "model_structure.json",
      "optimization": "optimization.json"
    },
    "domain": "Global",
    "name": "Trautmann2022_Opti",
    "time": {
      "date_begin": "2015-01-01",
      "date_end": "2019-12-31",
      "temporal_resolution": "day",
      "timesteps_in_day": 1,
      "timesteps_in_year": 365
    }
  },
  "exe_rules": {
    "data_type": "Float32",
    "input_array_type": "keyed_array",
    "input_data_backend": "netcdf",
    "land_output_type": "array",
    "model_array_type": "static_array",
    "parallelization": "threads",
    "tolerance": 1.0e-3,
    "yax_max_cache": 2e9
  },
  "flags": {
    "calc_cost": true,
    "catch_model_errors": true,
    "debug_model": false,
    "inline_update": false,
    "run_forward": true,
    "run_optimization": false,
    "save_info": true,
    "spinup": {
      "load_spinup": false,
      "run_spinup": true,
      "save_spinup": false,
      "spinup_TEM": true,
      "store_spinup": false
    },
    "use_forward_diff": false
  },
  "model_output": {
    "all": true,
    "depth_dimensions": {
      "d_ground": "groundW",
      "d_snow": "snowW",
      "d_soil": "soilW",
      "d_surface": "surfaceW"
    },
    "format": "nc",
    "output_array_type": "array",
    "path": null,
    "save_single_file": false,
    "variables": {
      "fluxes.evapotranspiration": null,
      "fluxes.evaporation": null,
      "fluxes.gpp": null,
      "fluxes.gw_recharge": null,
      "fluxes.runoff": null,
      "fluxes.sublimation": null,
      "fluxes.surface_runoff": null,
      "fluxes.transpiration": null,
      "pools.groundW": "d_ground",
      "pools.snowW": "d_snow",
      "pools.soilW": "d_soil",
      "pools.surfaceW": "d_surface",
      "states.fAPAR": null,
      "states.EVI": null,
      "states.water_balance": null
    }
  },
  "model_spinup": {
    "differential_eqn": {
      "absolute_tolerance": 10.0,
      "pools": ["cEco"],
      "relative_tolerance": 1e-2,
      "time_jump": 200.0
    },
    "paths": {
      "restart_file_in": null,
      "restart_file_out": null
    },
    "sequence": [
      {
        "forcing": "all_years",
        "n_repeat": 3,
        "spinup_mode": "all_forward_models",
        "stop_function": null
      }
    ]
  }
}
