{

	"time":
	{
		"step": 				"daily",
		"sDate": 				"1979-01-01",
		"eDate": 				"2017-12-31",
		"nYears": 				12,
		"nStepsDay": 			1,
		"nStepsYear": 			365
	},
	"mapping": {
		"yaxarray": ["longitude", "latitude"],
		"runEcosystem": ["time"],
		"parallelization": "threads"
	},
	"flags":
	{
		"saveInfo": 			true,
		"debugit": 				false, 
		"catchErrors": 			true,
		"runForward": 			true,
		"runUpdateModels": 		false,
		"runOpti": 				true,
		"calcCost":				true
	},
	"rules":
	{
		"verbosity": 5,
		"model_array_type": "staticarray",
		"forward_diff": false,
		"data_type": "Float64",
		"data_backend": "yaxarray",
		"tolerance": 1.0e-3,
		"yax_max_cache": 2e9
	},
	"output": {
		"output_array_type": "array",		
		"all": true,
		"path": null,
		"format": ".zarr",
		"depth_dimensions":{
			"d_soil": "soilW",
			"d_cEco": 8,
			"d_snow": "snowW"
		},
		"variables": {
			"fluxes.evapotranspiration": null,
			"fluxes.transpiration": null,
			"fluxes.runoff": null,
			"fluxes.runoffSurface": null,
			"fluxes.gpp": null,
			"fluxes.NEE": null,
			"pools.snowW": "d_snow",
			"pools.soilW": "d_soil",
			"pools.cEco": "d_cEco",
			"waterBalance.waterBalance": null
		}
	}
}
