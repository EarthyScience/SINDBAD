{
  "algorithm": "CMAEvolutionStrategy_CMAES",
  "land_output_type": "array",
  "model_parameter_default": {
    "distribution": ["normal", [0, 1]],
    "is_ml": false
  },
  "model_parameters_to_optimize": [
    "cCycleBase,c_τ_Root",
    "cCycleBase,c_τ_Wood",
    "cCycleBase,c_τ_LitSlow",
    "cCycleBase,c_τ_LitFast",
    "cCycleBase,c_τ_SoilSlow",
    "cCycleBase,c_τ_SoilOld",
    "cCycleBase,ηH",
    "cFlow,f_τ",
    "cFlow,k_shedding",
    "cFlow,slope_leaf_root_to_reserve",
    "cFlow,slope_reserve_to_leaf_root",
    "cTauSoilT,Q10",
    "cTauSoilW,Wopt",
    "evaporation,α",
    "evaporation,k_evaporation",
    "fAPAR,kEffExt",
    "gppPotential,εmax",
    "gppAirT,Topt",
    "gppAirT,ToptA",
    "gppAirT,ToptB",
    "gppVPD,κ",
    "gppVPD,Cκ",
    "gppVPD,Cm",
    "gppSoilW,q",
    "gppSoilW,θstar",
    "groundWRecharge,dos_exp",
    "autoRespiration,YG",
    "autoRespiration,RMN",
    "autoRespirationAirT,Q10",
    "runoffBase,bc",
    "rootWaterEfficiency,k_efficiency_cVegRoot",
    "rootMaximumDepth,constant_frac_max_root_depth",
    "runoffSaturationExcess,β",
    "snowMelt,melt_T",
    "snowMelt,melt_Rn",
    "treeFraction,constant_frac_tree",
    "WUE,WUEatOnehPa",
    "WUE,κ"
  ],
  "multi_constraint_method": "cost_sum",
  "multi_objective_algorithm": false,
  "observational_constraints": [
    "gpp",
    "nee",
    "transpiration",
    "evapotranspiration"
  ],
  "observations": {
    "default_cost": {
      "aggr_func": "nanmean",
      "aggr_obs": false,
      "aggr_order": "time_space",
      "cost_metric": "NSE_inv",
      "cost_weight": 1.0,
      "min_data_points": 1,
      "spatial_data_aggr": "concat_data",
      "spatial_cost_aggr": "spatially_variable",
      "spatial_weight": false,
      "temporal_data_aggr": "day"
    },
    "default_observation": {
      "additive_unit_conversion": false,
      "bounds": null,
      "data_path": "../data/BE-Vie.1979.2017.daily.nc",
      "is_categorical": false,
      "standard_name": null,
      "sindbad_unit": null,
      "source_product": "",
      "source_to_sindbad_unit": 1.0,
      "source_unit": null,
      "source_variable": null,
      "space_time_type": "spatiotemporal"
    },
    "use_quality_flag": true,
    "use_uncertainty": false,
    "use_spatial_weight": false,
    "variables": {
      "agb": {
        "cost_options": {
          "cost_metric": "NMAE1R",
          "temporal_data_aggr": "mean",
          "aggr_obs": true
        },
        "data": {
          "bounds": [0.0, 100000.0],
          "standard_name": "Above-ground biomass",
          "sindbad_unit": "gC m-2",
          "source_unit": "gC m-2",
          "source_variable": "agb_merged"
        },
        "model_full_var": "deriveVariables.aboveground_biomass",
        "unc": {
          "bounds": [0.0, 400.0],
          "data_path": null,
          "source_variable": "NEE_RANDUNC"
        }
      },
      "evapotranspiration": {
        "cost_options": {
          "cost_metric": "NSE_inv"
        },
        "data": {
          "bounds": [0.0, 100.0],
          "is_categorical": false,
          "standard_name": "Evapotranspiration",
          "sindbad_unit": "mm day-1",
          "source_to_sindbad_unit": 0.4081632653,
          "source_unit": "MJ m-2 d-1",
          "source_variable": "LE"
        },
        "model_full_var": "fluxes.evapotranspiration",
        "qflag": {
          "bounds": [0.85, 1.0],
          "data_path": null,
          "source_variable": "LE_QC_merged"
        },
        "unc": {
          "bounds": [0.0, 100.0],
          "data_path": null,
          "source_to_sindbad_unit": 0.4081632653,
          "source_variable": "LE_RANDUNC"
        }
      },
      "gpp": {
        "cost_options": {
          "cost_metric": "NSE_inv"
        },
        "data": {
          "bounds": [0.0, 100.0],
          "standard_name": "Gross Primary Productivity",
          "sindbad_unit": "gC m-2 day-1",
          "source_unit": "gC m-2 day-1",
          "source_variable": "GPP_NT"
        },
        "model_full_var": "fluxes.gpp",
        "qflag": {
          "bounds": [0.85, 1.0],
          "data_path": null,
          "source_variable": "GPP_QC_NT_merged"
        },
        "unc": {
          "bounds": [0.0, 100.0],
          "data_path": null,
          "source_variable": "NEE_RANDUNC"
        }
      },
      "ndvi": {
        "cost_options": {
          "cost_metric": "NSE_inv",
          "temporal_data_aggr": "day_anomaly",
          "aggr_obs": true
        },
        "data": {
          "bounds": [0.0, 1.0],
          "standard_name": "kNDVI",
          "sindbad_unit": "adimensional",
          "source_unit": "adimensional",
          "source_variable": "mergedNDVI_MCD43A"
        },
        "model_full_var": "states.fAPAR",
        "unc": {
          "bounds": [0.0, 1.0],
          "data_path": null,
          "source_variable": "NEE_RANDUNC"
        }
      },
      "nee": {
        "cost_options": {
          "cost_metric": "NSE_inv"
        },
        "data": {
          "bounds": [-100.0, 100.0],
          "standard_name": "Net Ecosystem Exchange",
          "sindbad_unit": "gC m-2 day-1",
          "source_unit": "gC m-2 day-1",
          "source_variable": "NEE"
        },
        "model_full_var": "fluxes.nee",
        "qflag": {
          "bounds": [0.85, 1.0],
          "data_path": null,
          "source_variable": "NEE_QC_merged"
        },
        "unc": {
          "bounds": [0.0, 400.0],
          "data_path": null,
          "source_variable": "NEE_RANDUNC"
        }
      },
      "reco": {
        "cost_options": {
          "cost_metric": "NSE_inv"
        },
        "data": {
          "bounds": [0.0, 100.0],
          "standard_name": "Ecosystem Respiration",
          "sindbad_unit": "gC m-2 day-1",
          "source_unit": "gC m-2 day-1",
          "source_variable": "RECO_NT"
        },
        "model_full_var": "fluxes.eco_respiration",
        "qflag": {
          "bounds": [0.85, 1.0],
          "data_path": null,
          "source_variable": "RECO_QC_NT_merged"
        },
        "unc": {
          "bounds": [0.0, 100.0],
          "data_path": null,
          "source_variable": "NEE_RANDUNC"
        }
      },
      "transpiration": {
        "cost_options": {
          "cost_metric": "NSE_inv"
        },
        "data": {
          "bounds": [0.0, 100.0],
          "standard_name": "transpiration",
          "sindbad_unit": "mm day-1",
          "source_unit": "mm day-1",
          "source_variable": "T_NT_TEA"
        },
        "model_full_var": "fluxes.transpiration",
        "qflag": {
          "bounds": [0.85, 1.0],
          "data_path": null,
          "source_variable": "LE_QC_merged"
        },
        "unc": {
          "bounds": [0.0, 100.0],
          "data_path": null,
          "source_to_sindbad_unit": 0.4081632653,
          "source_variable": "LE_RANDUNC"
        }
      }
    }
  }
}
