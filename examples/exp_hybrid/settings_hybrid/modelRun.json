{

	"time":
	{
		"model_time_step": 			"daily",
		"start_date": 				"2005-01-01",
		"end_date": 				"2017-12-31",
		"timesteps_in_day": 		1,
		"timesteps_in_year": 		365.0
	},
	"mapping": {
		"yaxarray": [],
		"run_ecosystem": ["time", "site"],
		"parallelization": "threads"
	},
	"flags":
	{
		"save_info": 			true,
		"debug_model": 			false, 
		"catch_model_errors": 	false,
		"run_spinup": 			true,
		"run_forward": 			true,
		"run_update_models": 	false,
		"run_optimization": 	true,
		"run_forward_and_cost":	true
	},
	"rules":
	{
		"verbosity": 5,
		"model_array_type": "staticarray",
		"forward_diff": true,
		"data_type": "Float32",
		"data_backend": "zarr",
		"tolerance": 1.0e-3,
		"yax_max_cache": 2e9
	},
	"output": {
		"output_array_type": "array",		
		"all": false,
		"path": null,
		"format": ".zarr",
		"depth_dimensions":{
			"d_soil": "soilW",
			"d_cEco": 8,
			"d_snow": "snowW"
		},
		"variables": {
			"fluxes.evapotranspiration": null,
			"fluxes.transpiration": null,
			"fluxes.runoff": null,
			"fluxes.surface_runoff": null,
			"fluxes.gpp": null,
			"pools.snowW": "d_snow",
			"pools.soilW": "d_soil",
			"waterBalance.water_balance": null
		}
	}
}
