{
	"method": {
		"lsqnonlin": {
			"Algorithm": "levenberg-marquardt",
			"LevenbergMarquardt": true,
			"LargeScale": false,
			"Jacobian": false,
			"ScaleProblem": false,
			"DiffMinChange": [0.001],
			"DiffMaxChange": [Inf],
			"MaxFunEvals": [10000],
			"MaxIter": [10000],
			"TolX": [1.00E-12],
			"TolFun": [1.00E-12],
			"Display": false
		},
		"cmaes": {
            "MaxFunEvals"           : 10000,
            "MaxIter"               : "1e3*(N+5)^2/sqrt(popsize);",
            "PopSize"               : "3 * (4 + floor(3*log(N)));",
            "StopFunEvals"          : 1e+06,
            "StopIter"              : 1e+06,
            "TolX"                  : 1e-6,
            "TolFun "               : 1e-6,
            "StopFitness"           : 1e-03,
            "LBounds"               : "info.opti.params.lBoundsScaled;", 
            "UBounds"               : "info.opti.params.uBoundsScaled;",
            "StopOnWarnings"        : "no;",
            "SaveVariables"         : "on;",
            "SaveFilename"          : "variablescmaes.mat",
            "LogFilenamePrefix"	    : "outcmaes",
            "LogPlot"               : "off;",
            "UserData"              : ";",
            "DiagonalOnly"          : 1, 
            "CMA.active"            : 2, 
            "pSigma"                : []
		}
	}
}
